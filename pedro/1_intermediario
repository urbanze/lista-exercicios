#include <stdio.h>
#include <math.h>
#include <ctype.h>
#include <string.h>

int main() {
    int n;
    int m;
    scanf ("%d", &n);
    
    int tabela[n][5];
    
    for (int i = 0; i < n; i++){
        for (int j = 0; j < 5; j++){
            scanf ("%d", &tabela[i][j]);
        }
    }
      
    for (int i = 0; i < n; i++){
        m = tabela[i][0];
        for (int j = 1; j < 5; j++){
            if (tabela[i][j] > m){
                m = tabela[i][j];
            }
        }
    }  
      
    return 0;
}
