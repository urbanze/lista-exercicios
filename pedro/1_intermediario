#include <stdio.h>
#include <math.h>
#include <ctype.h>
#include <string.h>

int var1() {
    int n;
    int nn;
    int m;
    int idx;
    int contador;
    
    scanf ("%d", &n);
    scanf ("%d", &nn);
    
    int cont[n];
    int tabela[n][nn];
    int aux [n][nn];
    
    for (int i = 0; i < n; i++){
        contador = 0;
        for (int j = 0; j < nn; j++){
            scanf ("%d", &tabela[i][j]);
            if (tabela[i][j] == 2){
                contador = contador + 1;
            }
            else{
                for (int k = 2; k < tabela[i][j]; k++){
                    if (tabela[i][j] % k == 0){
                        break;
                    }
                    if (k == tabela[i][j] - 1){
                        contador = contador + 1;
                    }
                }
            }
        }
        cont[i] = contador;
    }
      
    for (int i = 0; i < n; i++){
        for (int j = 0; j < nn; j++){
            for (int k = 0; k < n; k++){
                if (k == 0){
                    m = tabela[i][0];
                    idx = 0;
                }
                if (i % 2 == 1){
                    if (tabela[i][k] > m){
                        m = tabela[i][k];
                        idx = k;
                    }
                }
                else{
                    if (tabela[i][k] < m){
                        m = tabela[i][k];
                        idx = k;
                    }
                }
                if (k == nn - 1){
                    aux[i][j] = m;
                    if (i % 2 == 1){
                        tabela[i][idx] = 0;
                    }
                    else{
                        for (int l = 0; l < nn; l++){
                            tabela[i][idx] = tabela[i][idx] + tabela[i][l];
                        }
                    }
                }
            }
        }
    }
    
    for (int i = 0; i < n; i++){
        for (int j = 0; j < nn; j++){
            printf ("%d\n", aux[i][j]);
        }
    }
      
    return 0;
}

int var2() {
    int n;
    int nn;
    int m;
    int idx;
    int contador;
    
    scanf ("%d", &n);
    scanf ("%d", &nn);
    
    int cont[n];
    int tabela[n][nn];
    int aux [n][nn];
    
    for (int i = 0; i < n; i++){
        contador = 0;
        for (int j = 0; j < nn; j++){
            scanf ("%d", &tabela[i][j]);
            if (tabela[i][j] == 2){
                contador = contador + 1;
            }
            else{
                for (int k = 2; k < tabela[i][j]; k++){
                    if (tabela[i][j] % k == 0){
                        break;
                    }
                    if (k == tabela[i][j] - 1){
                        contador = contador + 1;
                    }
                }
            }
        }
        cont[i] = contador;
    }
      
    for (int i = 0; i < n; i++){
        for (int j = 0; j < nn; j++){
            for (int k = 0; k < nn; k++){
                if (k == 0){
                    m = tabela[i][0];
                    idx = 0;
                }
                if (cont[i] == 1){
                    if (tabela[i][k] > m){
                        m = tabela[i][k];
                        idx = k;
                    }
                }
                if (cont[i] == 0){
                    if (tabela[i][k] < m){
                        m = tabela[i][k];
                        idx = k;
                    }
                }
                if (cont[i] > 1){
                    aux[i][k] = tabela[i][k];
                }
                if (k == nn - 1 && cont[i] <= 1){
                    aux[i][j] = m;
                    if (cont[i] == 1){
                        tabela[i][idx] = 0;
                    }
                    if (cont[i] == 0){
                        for (int l = 0; l < nn; l++){
                            tabela[i][idx] = tabela[i][idx] + tabela[i][l];
                        }
                    }
                }
            }
        }
    }
    
    for (int i = 0; i < n; i++){
        for (int j = 0; j < nn; j++){
            printf ("%d\n", aux[i][j]);
        }
    }
      
    return 0;
}

int main(){
    
    var1();
    var2();
    
    return 0;
}
